{
  "name": "whatwg-encoding",
  "description": "Decode strings according to the WHATWG Encoding Standard",
  "keywords": [
    "encoding",
    "whatwg"
  ],
  "version": "2.0.0",
  "author": "Domenic Denicola <d@domenic.me> (https://domenic.me/)",
  "license": "MIT",
  "repository": "jsdom/whatwg-encoding",
  "main": "lib/whatwg-encoding.js",
  "files": [
    "index.js",
    "webidl2js-wrapper.js",
    "lib/"
  ],
  "scripts": {
    "build": "node scripts/update.js && node scripts/convert-idl.js",
    "prepare": "node scripts/update.js && node scripts/convert-idl.js",
    "pretest": "node scripts/get-latest-platform-tests.js && node scripts/transform.js && node scripts/convert-idl.js",
    "build-live-viewer": "browserify index.js --standalone whatwgEncoding > live-viewer/whatwg-encoding.js",
    "test": "mocha",
    "lint": "eslint ."
  },
  "dependencies": {
    "iconv-lite": "0.6.3"
    "text-encoding": "0.7.0",
    "webidl2js": "15.1.0"
  },
  "devDependencies": {
    "@domenic/eslint-config": "^1.3.0",
    "eslint": "^7.32.0",
    "got": "^9.0.0",
    "minipass-fetch": "^1.4.1",
    "mocha": "^9.1.1"
  },
  "engines": {
    "node": ">=12"
  }
}